<h2>user#edit</h2>
<div class="user-edit">
  <div class="user-edit-title">
    <h2>User info</h2>
  </div>
  <%= form_with model:@user, local:true do |f| %>
  <label for = "inputNama">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputNama", placheholder:"名前" %>
  <p>Image<%= attachment_image_tag @user, :profile_image, fallback:"no_image.jpg", size:"60x60" %></p>
  <label for ="introduction">introduction</label>
  <%= f.text_area :body,autofocus: true, id:"introduction", placheholder:"説明" %>
  <%= f.submit "UpdateUser" %>
  <% end %>
</div>